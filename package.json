        "tauri-plugin-store-api": "github:tauri-apps/tauri-plugin-store#8a37f08e6c0b7781fc1f0e458f2d0ff18368ac75",
        "vue3-smooth-dnd": "0.0.5",
        "zod": "^3.20.6"
    },
    "resolutions": {
        "terser": "5.19.0",
        "undici": "5.22.1",
        "loader-utils": "2.0.4",
        "json5": "2.2.3"
    }
}
