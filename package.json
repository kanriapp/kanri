{
    "name": "kanri",
    "private": true,
    "version": "0.2.2",
    "description": "Offline-first Kanban board desktop app",
    "author": "trobonox <hello@trobo.tech>",
    "license": "Apache-2.0",
    "scripts": {
        "init:armv7": "docker build . -t rust_cross_compile/armv7 -f Dockerfile.armv7",
        "init:arm64": "docker build . -t rust_cross_compile/arm64 -f Dockerfile.arm64",
        "build:armv7": "nuxt generate && docker run -t -v `pwd`:/app rust_cross_compile/armv7",
        "build:arm64": "nuxt generate && docker run -t -v `pwd`:/app rust_cross_compile/arm64",
        "dev": "nuxt dev",
        "generate": "nuxt generate",
        "preview": "nuxt preview",
        "lint": "eslint . --fix",
        "prebuild": "node buildremind.js"
    },
    "devDependencies": {
        "@nuxt/eslint-config": "0.1.1",
        "@nuxtjs/tailwindcss": "6.2.0",
        "@tauri-apps/cli": "1.2.2",
        "eslint": "8.31.0",
        "eslint-plugin-tailwindcss": "^3.8.0",
        "nuxt": "3.0.0",
        "postcss-custom-properties": "12.1.11"
    },
    "dependencies": {
        "@heroicons/vue": "2.0.13",
        "@pinia/nuxt": "0.4.6",
        "@tauri-apps/api": "1.2.0",
        "floating-vue": "2.0.0-beta.20",
        "mitt": "3.0.0",
        "pinia": "2.0.28",
        "tauri-plugin-store-api": "github:tauri-apps/tauri-plugin-store#9bd993aa67766596638bbfd91e79a1bf8f632014",
        "tauri-plugin-autostart-api": "github:tauri-apps/tauri-plugin-autostart#031efac82d592774c986e94c6258a3dc11f54d81",
        "vue3-smooth-dnd": "0.0.2"
    },
    "resolutions": {
        "terser": "5.16.1",
        "undici": "5.15.0",
        "loader-utils": "2.0.4",
        "json5": "2.2.3"
    }
}
